{
  "name": "trello-connect",
  "version": "2.0.6",
  "description": "Lightweight client for connecting to Trello",
  "main": "dist/trello-connect.min.js",
  "module": "index.js",
  "scripts": {
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "build:all": "$npm_execpath run build:dev && $npm_execpath run build:prod",
    "serve": "webpack-dev-server --config webpack.dev.js --open"
  },
  "keywords": [
    "Trello",
    "Project Monitoring"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/charles-owen/trello-connect.git"
  },
  "author": "Charles B. Owen",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/runtime": "7.16.5",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "9.1.0",
    "html-webpack-plugin": "^5.5.0",
    "resolve-url-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "4.7.1",
    "webpack-merge": "^5.8.0"
  }
}
