{
  "name": "trello-connect",
  "version": "2.0.3",
  "description": "Lightweight client for connecting to Trello",
  "main": "dist/trello-connect.min.js",
  "module": "index.js",
  "private": false,
  "scripts": {
    "build:dev": "npx webpack --config webpack.dev.js",
    "build:prod": "npx webpack --config webpack.prod.js",
    "build:all": "npm run build:dev && npm run build:prod",
    "serve": "webpack-dev-server --config webpack.dev.js --open"
  },
  "keywords": [
    "Trello",
    "Project Monitoring"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/charles-owen/trello-connect.git"
  },
  "author": "Charles B. Owen",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "html-webpack-include-assets-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "resolve-url-loader": "^3.1.0",
    "terser-webpack-plugin": "^1.4.1",
    "url-loader": "^2.1.0",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.5"
  },
  "dependencies": {}
}
