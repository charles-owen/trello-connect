{
  "name": "trello-connect",
  "version": "2.0.4",
  "description": "Lightweight client for connecting to Trello",
  "main": "dist/trello-connect.min.js",
  "module": "index.js",
  "private": false,
  "scripts": {
    "build:dev": "npx webpack --config webpack.dev.js",
    "build:prod": "npx webpack --config webpack.prod.js",
    "build:all": "npm run build:dev && npm run build:prod",
    "serve": "webpack-dev-server --config webpack.dev.js --open"
  },
  "keywords": [
    "Trello",
    "Project Monitoring"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/charles-owen/trello-connect.git"
  },
  "author": "Charles B. Owen",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/runtime": "7.12.13",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "html-webpack-plugin": "^5.1.0",
    "resolve-url-loader": "^3.1.2",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.21.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "4.0.0-beta.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {}
}
